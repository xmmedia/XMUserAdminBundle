                <source>xm_user_admin.parameter.name_full</source>
                <target>Company Name</target>
            </trans-unit>

            <!-- ****** MESSAGES ****** -->
            <trans-unit id="xm_user_admin.message.entity_updated">
                <source>xm_user_admin.message.entity_updated</source>
                <target>The %name% has been updated.</target>
            </trans-unit>
            <trans-unit id="xm_user_admin.message.entity_deleted">
                <source>xm_user_admin.message.entity_deleted</source>
