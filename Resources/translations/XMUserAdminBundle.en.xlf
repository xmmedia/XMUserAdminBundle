                <source>xm_user_admin.message.user.locked</source>
                <target>The user's account has been locked. They will no longer be able to use their account.</target>
            </trans-unit>
            <trans-unit id="xm_user_admin.message.user.created">
                <source>xm_user_admin.message.user.created</source>
                <target>The user was created and a welcome email was sent with a link to set their password and activate their account.</target>
            </trans-unit>
            <trans-unit id="xm_user_admin.message.user.created_set_password">
                <source>xm_user_admin.message.user.created_set_password</source>
                <target>The user was created and a welcome email was sent. Please let them know the password you entered.</target>
            </trans-unit>
            <trans-unit id="xm_user_admin.message.user.cant_delete">
                <source>xm_user_admin.message.user.cant_delete</source>
                <target>The user cannot be deleted.</target>
            </trans-unit>

