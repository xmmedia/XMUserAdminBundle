{
    "name": "xm/user-admin-bundle",
    "description": "Provides a simple user admin.",
    "type": "symfony-bundle",
    "homepage": "https://github.com/xmmedia/XMUserAdminBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Darryl Hein",
            "email": "dhein@xmmedia.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "symfony/form": "~3.0",
        "symfony/framework-bundle": "~3.0",
        "doctrine/orm": "~2.5",
        "xm/filter-bundle": "dev-master",
        "xm/mail-manager-bundle": "dev-master"
    },
    "autoload": {
        "psr-4": { "XM\\UserAdminBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:xmmedia/XMFilterBundle.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:xmmedia/XMMailManagerBundle.git"
        }
    ]
}
